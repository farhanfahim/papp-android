<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_gray"
    android:orientation="vertical">


    <include layout="@layout/title_clinical_lab" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"

        android:background="@color/background_gray" />

    <edu.aku.akuh_health_first.widget.AnyTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#F0F6E7"
        android:gravity="center_horizontal"
        android:padding="@dimen/x3dp"
        android:text="RESULTS"
        android:textColor="@color/base_amber"
        android:textSize="@dimen/s14"
        app:typeface="@string/font_bold" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/listClinicalLabResult"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/x5dp" />


</LinearLayout>